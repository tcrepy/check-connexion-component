{
  "name": "wcomponent-check-connexion",
  "version": "0.2.1",
  "description": "web component that is used to check user connexion",
  "main": "src/index.js",
  "author": "tcrepy",
  "repository": "github:tcrepy/check-connexion-component",
  "license": "ISC",
  "dependencies": {
    "lit-element": "^2.1.0"
  },
  "keywords": [
    "web component",
    "connexion",
    "check connexion"
  ],
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "pika-plugin-minify",
        {
          "minifyOptions": {
            "ecma": 5,
            "keep_classnames": false,
            "keep_fnames": false,
            "ie8": true,
            "module": true,
            "safari10": true,
            "toplevel": false,
            "warnings": false
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "@pika/plugin-build-node": "^0.3.16",
    "@pika/plugin-build-web": "^0.3.16",
    "@pika/plugin-standard-pkg": "^0.3.16",
    "pika-plugin-minify": "^0.1.0"
  }
}
